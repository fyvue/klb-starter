{
  "name": "demo",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --force --port 3000",
    "build": "yarn run build:client && yarn run build:server:noExternal",
    "build:client": "vue-tsc --noEmit && vite build --ssrManifest --outDir dist/client",
    "build:server": "vue-tsc --noEmit && vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:server:noExternal": "vue-tsc --noEmit && vite build --config vite.config.noexternal.ts --ssr src/entry-server.ts --outDir dist/server",
    "typecheck": "vue-tsc --noEmit",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix  --ignore-path .gitignore"
  },
  "dependencies": {
    "@fy-/components": "^0.2.80",
    "@fy-/core": "^0.0.20",
    "@fy-/head": "^0.0.37",
    "@fy-/icons": "^0.0.2",
    "@headlessui/vue": "^1.7.14",
    "@karpeleslab/i18next-klb-backend": "^0.1.0",
    "@karpeleslab/klbfw": "^0.1.13",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.3",
    "@vueuse/core": "^10.2.1",
    "i18next": "^23.2.11",
    "pinia": "^2.1.4",
    "timeago.js": "^4.0.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue3-lazyload": "^0.3.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@heroicons/vue": "^2.0.18",
    "@rollup/plugin-replace": "^5.0.2",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/youtube": "^0.0.47",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "express": "^4.18.2",
    "postcss": "^8.4.26",
    "prettier": "3.0.0",
    "sass": "^1.63.6",
    "tailwind-scrollbar": "^3.0.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vue-tsc": "^1.8.5"
  }
}
